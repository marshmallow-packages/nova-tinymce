{
    "name": "marshmallow/nova-tinymce",
    "description": "This Nova package allow you to use TinyMCE editor for text areas.You can customize the editor options and... you can upload images to your server and put them rigth there on the text without leaving the text editor!",
    "keywords": [
        "laravel",
        "nova",
        "editor",
        "tiny"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Nova\\TinyMCE\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Nova\\TinyMCE\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
